<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="space" prefHeight="160.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rm.controller.NotificationsController">
    <HBox prefHeight="160.0" prefWidth="650.0">
        <Label fx:id="notificationText" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="650.0" textFill="WHITE" />
        <FontAwesomeIcon iconName="MINUS" onMouseClicked="#close">
            <HBox.margin>
                <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
            </HBox.margin>
        </FontAwesomeIcon>
        <FontAwesomeIcon iconName="CLOSE" onMouseClicked="#closeAll">
            <HBox.margin>
                <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
            </HBox.margin>
        </FontAwesomeIcon>
    </HBox>
</AnchorPane>
